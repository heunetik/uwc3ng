// Uwc3ng include file

public UWC3NG_Death( iAttacker, iVictim, iHeadshot, iWeaponID )
{
	if ( !is_user_connected( iAttacker ) || !is_user_connected( iVictim ) )
	{
		return;
	}
	
	// The player is dead!
	p_data_b[iVictim][PB_DIEDLASTROUND] = true;
}
