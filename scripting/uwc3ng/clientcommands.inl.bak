// Uwc3ng clientcommands functions

// Someone want the skill menu
public COMMAND_SkillMenu( id )
{
	if ( !UWC3NG_Check() )
	{
		return;
	}
	
	// Open the skillmenu
	SKILL_ShowMenu( id, 0 );
	
	return;
}
